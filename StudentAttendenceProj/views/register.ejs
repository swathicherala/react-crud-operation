<div>
    <table class="table table-dark table-bordered" id="display">
        <thead>
            <tr>
                <th>Student Id</th>
                <th>Studenr Firstname</th>
                <th>Studenr Lastname</th>
                <th>Course</th>
            </tr>
        </thead>
        <tbody>
            <% course.forEach(function(studentdetails){
              %>
              <tr>
                  <td><%= studentdetails.id%></td>
                  <td><%= studentdetails.firstname%></td>
                  <td><%= studentdetails.lastname%></td>
                  <td><%= studentdetails.course%></td>
              </tr>
            <%});
            %>
        </tbody>
    </table>
</div>